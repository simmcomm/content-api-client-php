{
    "name": "simmcomm/content-api-client",
    "description": "Content API client",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ivan Pepelko",
            "email": "ivan.pepelko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Flowly\\Content\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flowly\\Content\\ApiClientTest\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/annotations": "^1.11",
        "doctrine/cache": "^1.10 || ^2.0",
        "psr/log": "^1.1",
        "simmcomm/content-api-common": "^1.5",
        "symfony/cache": "^5.2",
        "symfony/cache-contracts": "^2.2",
        "symfony/http-client": "^5.2",
        "symfony/polyfill-php80": "^1.22",
        "symfony/property-access": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/validator": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    }
}
